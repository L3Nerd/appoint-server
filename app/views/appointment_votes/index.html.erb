<p id="notice"><%= notice %></p>

<h1>Listing Appointment Votes</h1>

<table>
  <thead>
    <tr>
      <th>State</th>
      <th>Appointment</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @appointment_votes.each do |appointment_vote| %>
      <tr>
        <td><%= appointment_vote.state %></td>
        <td><%= appointment_vote.appointment %></td>
        <td><%= appointment_vote.user %></td>
        <td><%= link_to 'Show', appointment_vote %></td>
        <td><%= link_to 'Edit', edit_appointment_vote_path(appointment_vote) %></td>
        <td><%= link_to 'Destroy', appointment_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Appointment vote', new_appointment_vote_path %>
